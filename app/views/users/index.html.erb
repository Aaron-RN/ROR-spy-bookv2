<h1> This is the index page </h1>

<h2>All Users</h2>
<% @users.each do |user|%>
  <%= link_to user_path(user) do %>
    <%= user.fname %> <%= user.lname %>
  <% end %>
  <br>
  <% if current_user.id != user.id%>
    <%= link_to '#' do %>
      <button class= "btn btn-warning"> Send Friend Request? </button>
    <% end %>
  <% end %>
  <br><br>
<% end %>